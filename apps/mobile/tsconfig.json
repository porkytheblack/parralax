  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@theme/*": ["./ui/theme/*"],
      "@components/*": ["./ui/components/*"],
      "@screens/*": ["./ui/screens/*"],
      "@navigation/*": ["./navigation/*"],
      "@hooks/*": ["./hooks/*"],
      "@utils/*": ["./utils/*"],
